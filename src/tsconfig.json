{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": false,
        "target": "es5",
        "outDir": "js",
        "module": "system",
        "jsx": "react"
    },
    "compileOnSave": true,
    "excludes": [],
    "files": [
        "ts/api.ts",
        "ts/app.tsx"
    ],
    "filesGlob": [
        "ts/*.ts",
        "ts/*.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
